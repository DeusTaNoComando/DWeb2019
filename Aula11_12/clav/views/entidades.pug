
extends layout

block content
  .w3-container
    h1 Lista de Entidades
    table.w3-table-all
        tr
            th Sigla
            th Estado
            th Designação
            th Ação
        each ent in entidades
            tr
                td= ent.sigla
                td= ent.estado
                td= ent.designacao
                td
                    button.w3-btn(onclick="window.location.assign('/" + ent.id + "')" class='fa fa-edit')