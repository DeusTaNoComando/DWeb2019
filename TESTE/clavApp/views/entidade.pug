extends layout
block content
    each e in entidade
        .w3-container
            header.w3-container.w3-red
                h1 Entidade: #{e.sigla}
            div.w3-container
                p= e.sigla
                br
                a(href= e.id)= e.id
                br
                a(href= 'http://localhost:3000/'+e.id+'/tipologias') Tipologias
            footer.w3-container.w3-red
                a(href='http://localhost:3000/') Voltar à página inicial